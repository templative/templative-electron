{
    "name": "Templative",
    "description": "Electron frontend for the Templative CLI.",
    "version": "1.0.73",
    "private": true,
    "main": ".webpack/main",
    "author": {
        "name": "Go Next Games",
        "email": "oliver@gonextgames.com",
        "url": "https://www.gonextgames.com"
    },
    "scripts": {
        "== Electron App ===": "",
        "start": "electron-forge start",
        "package": "bash ./build.sh package",
        "package-local": "SKIP_SIGNING=true bash ./build.sh package",
        "make": "bash ./build.sh make",
        "publish": "bash ./build.sh publish",
        "== Webpack Debugging ===": "",
        "webpack-debug": "NODE_ENV=development DEBUG=webpack:* electron-forge start",
        "webpack-stats": "webpack --config webpack.main.config.js --profile --json > webpack-stats.json",
        "== Templative CLI ===": "",
        "templative": "bash ./build.sh templative",
        "templative-build": "bash ./build.sh createTemplativeApp",
        "== Build Process ===": "",
        "makeFromPackage": "bash ./build.sh makeFromPackage",
        "== Signing & Publishing ===": "",
        "sign": "bash ./build.sh sign",
        "signS3": "bash ./signWindowsInS3.sh",
        "createPublishDryRun": "bash ./build.sh createPublishDryRun",
        "publishDryRun": "bash ./build.sh publishDryRun",
        "== Testing ===": "",
        "test": "node ./scripts/test-templative.js",
        "test-distribute": "node ./scripts/test-templative.js --input=/Users/oliverbarnum/Documents/git/apcw-defines",
        "test-webpack": "electron-forge start --no-launch && node ./scripts/test-templative.js",
        "test-webpack-distribute": "electron-forge start --no-launch && node ./scripts/test-templative.js --input=/Users/oliverbarnum/Documents/git/peace-of-westphalia",
        "test-giant-project": "node ./scripts/test-giant-project.js",
        "test-clipping": "node ./scripts/test-clipping.js",
        "== Asset Management ===": "",
        "analyze-images": "node ./scripts/imageTreeGenerator.js",
        "export-audit": "node ./scripts/exportAudit.js",
        "update-component-info": "node ./scripts/updateComponentInfoBasedOnRules.js",
        "design-meeple": "dotenv -- node ./scripts/meepleDesigner.js",
        "create-cylinders": "node ./scripts/cylinderCreator.js",
        "create-cylinder-objects": "node ./scripts/cylinderObjectCreator.js",
        "== SVG & Image Processing ===": "",
        "fix-svg-scaling": "node ./scripts/fix-svg-scaling.js",
        "fix-all-svgs": "node ./scripts/fix-svg-scaling.js --glob \"src/main/templative/lib/create/guidelines/*.svg\"",
        "download-preview-images": "node ./scripts/downloadPreviewImages.js",
        "download-stock-preview-images": "node ./scripts/downloadStockPreviewImages.js",
        "download-templates": "node ./scripts/downloadTemplates.js",
        "upload-preview-images": "node ./scripts/uploadPreviewImages.js",
        "// === Component Management ===": "",
        "standardize-filenames": "node ./scripts/standardizeGuidelineFilenames.js",
        "match-guidelines-to-components": "node ./scripts/matchGuidelinesToComponents.js",
        "sort-components-into-categories-colors-sizes": "node ./scripts/sortComponentsIntoCategoriesColorsSizes.js"
    },
